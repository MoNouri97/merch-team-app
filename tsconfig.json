{
	"include": ["./App.tsx", "./src/**/*"],
	"exclude": ["./babel.config.js"],
	"compilerOptions": {
		"removeComments": true,
		"forceConsistentCasingInFileNames": true,
		// "importHelpers": true,
		"target": "es5",
		"module": "commonjs",
		"jsx": "react-native",
		"lib": [
			"es6",
			"esnext.asynciterable"
		],
		"strict": true,
		"esModuleInterop": true,
		"rootDir": "src",
		"noImplicitReturns": true,
		"noImplicitThis": true,
		"noImplicitAny": true,
		"strictNullChecks": true,
		"suppressImplicitAnyIndexErrors": true,
		"noUnusedLocals": true,
		"skipLibCheck": true,
		"baseUrl": ".",
		"paths": {
			"~/*": ["src/*"],
		}
	}
}

